<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- Development dependencies - not used in production -->
    <suppress>
        <notes><![CDATA[
        webpack-dev-server is a development dependency only, not deployed to production.
        CVE-2025-30360 affects dev server only.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/webpack-dev-server@.*$</packageUrl>
        <cve>CVE-2025-30360</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
        html-minifier-terser is a build-time dependency used by react-scripts.
        CVE-2022-37620 affects build process only, not runtime.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/html-minifier-terser@.*$</packageUrl>
        <cve>CVE-2022-37620</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
        nth-check is a transitive dependency of css-select used in build tools.
        CVE-2021-3803 and GHSA-rp65-9cf3-cjxr affect build process only.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/nth-check@.*$</packageUrl>
        <cve>CVE-2021-3803</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
        nth-check GHSA suppression
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/nth-check@.*$</packageUrl>
        <vulnerabilityName>GHSA-rp65-9cf3-cjxr</vulnerabilityName>
    </suppress>

    <suppress>
        <notes><![CDATA[
        underscore.js is a transitive dependency. CVE-2021-23358 affects template function
        which is not used in this application. GHSA-cf4h-3jhx-xvhq is related.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/underscore@.*$</packageUrl>
        <cve>CVE-2021-23358</cve>
    </suppress>

    <suppress>
        <notes><![CDATA[
        underscore.js GHSA suppression
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/underscore@.*$</packageUrl>
        <vulnerabilityName>GHSA-cf4h-3jhx-xvhq</vulnerabilityName>
    </suppress>

    <suppress>
        <notes><![CDATA[
        underscore.js JavaScript file detection
        ]]></notes>
        <packageUrl regex="true">^pkg:javascript/underscore\.js@.*$</packageUrl>
        <cve>CVE-2021-23358</cve>
    </suppress>

</suppressions>

